{
	"name": "pipeline_2_PuntoA",
	"properties": {
		"description": "2. Requerimientos mínimos\nA. Creación de pipelines de cargue, transformación y cargue de información",
		"activities": [
			{
				"name": "Cargarolist_customers_dataset",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
